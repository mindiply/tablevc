{
  "name": "tablevc",
  "version": "0.0.7",
  "description": "Version control of tables of data",
  "main": "lib/index.js",
  "scripts": {
    "build": "run-p build:**",
    "build:transpile": "babel src --ignore \"src/**/.git*\" --extensions \".ts\" --out-dir lib --source-maps",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "eslint --ext .ts \"src/**\"",
    "prepublishOnly": "run-s lint test build",
    "test": "jest",
    "test-cover": "jest --coverage"
  },
  "keywords": [
    "synchronization",
    "version_control",
    "tables"
  ],
  "author": "Paolo F. Bongiovanni <paolo.bongiovanni@mindiply.com>",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.14",
    "@types/jest": "^26.0.22",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@types/randombytes": "^2.0.0",
    "jssha": "^3.2.0",
    "randombytes": "^2.1.0"
  }
}
